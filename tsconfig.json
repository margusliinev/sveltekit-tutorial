{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "forceConsistentCasingInFileNames": true,
        "noUncheckedIndexedAccess": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "sourceMap": true,
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "moduleResolution": "bundler"
    }
}
